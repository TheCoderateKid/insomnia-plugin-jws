{
  "name": "insomnia-plugin-jws",
  "version": "1.1.0",
  "description": "Insomnia plugin for generating JWS signatures with detached payload support",
  "main": "index.js",
  "author": "Rusty Shackleford",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "lint": "eslint index.js test/",
    "lint:fix": "eslint index.js test/ --fix",
    "validate": "node -e \"require('./index.js'); console.log('Plugin loads successfully');\""
  },
  "insomnia": {
    "name": "JWSPlugin",
    "displayName": "JWS Signature Generator",
    "description": "Generate JWS signatures (including detached payload) for API authentication"
  },
  "keywords": [
    "insomnia",
    "plugin",
    "jws",
    "jwt",
    "signature",
    "authentication",
    "api"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "node-forge": "^1.3.1"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "jest": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "index.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 45,
        "functions": 75,
        "lines": 60,
        "statements": 60
      }
    },
    "testMatch": [
      "**/test/**/*.test.js"
    ],
    "verbose": true
  }
}
